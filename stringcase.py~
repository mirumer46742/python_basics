def swap_case(string):
    newstring=""
    for s in string:
        if s.islower():
            newstring+=s.upper()
        elif s.isupper():
            newstring+=s.lower()
        else:
            newstring+=s
    return newstring 

string=raw_input("Enter the string in mixed case: ")
last=swap_case(string)
print "The string with changed case is: ",last
